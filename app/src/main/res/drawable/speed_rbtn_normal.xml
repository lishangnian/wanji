<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/speed_normal_no" android:state_checked="false">
        <shape android:shape="rectangle">
            <!--            边框颜色-->
            <corners android:radius="5px" />
            <!--            <solid android:color="@color/checkColorBlue" />-->
            <!-- 设置按钮的四个角为弧形 -->
            <!-- android:radius 弧形的半径 -->
            <stroke android:width="2px" android:color="@color/checkColorBlue" />
        </shape>
    </item>
    <item android:drawable="@drawable/speed_normal" android:state_checked="true">
        <shape android:shape="rectangle">
            <!--            边框颜色-->
            <corners android:radius="5px" />
            <!--            <solid android:color="@color/checkColorBlue" />-->
            <!-- 设置按钮的四个角为弧形 -->
            <!-- android:radius 弧形的半径 -->
            <stroke android:width="2px" android:color="@color/checkColorBlue" />
        </shape>
    </item>
    <!--    <item android:state_selected="true"  android:drawable="@drawable/buttonsubject_2"/>-->
    <!--    <item android:state_pressed="true"  android:drawable="@drawable/buttonsubject_2"/>-->

</selector>