<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:drawable="@drawable/speed_slow_no01">
        <shape android:shape="rectangle">
            <!--            边框颜色-->
            <stroke android:width="2px" android:color="@color/checkColorBlue" />
<!--            <solid android:color="@color/checkColorBlue" />-->
            <!-- 设置按钮的四个角为弧形 -->
            <!-- android:radius 弧形的半径 -->
            <corners android:radius="5px" />
        </shape>
    </item>
    <item android:state_checked="true"  android:drawable="@drawable/speed_slow01">
        <shape android:shape="rectangle">
            <!--            边框颜色-->
            <stroke android:width="2px" android:color="@color/checkColorBlue" />
<!--            <solid android:color="@color/checkColorBlue" />-->
            <!-- 设置按钮的四个角为弧形 -->
            <!-- android:radius 弧形的半径 -->
            <corners android:radius="5px" />
        </shape>
    </item>
<!--    <item android:state_selected="true"  android:drawable="@drawable/buttonsubject_2"/>-->
<!--    <item android:state_pressed="true"  android:drawable="@drawable/buttonsubject_2"/>-->

</selector>